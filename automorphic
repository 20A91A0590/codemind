#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,s,flag=0;//n=5 
    cin>>n;
    s=n*n;//s=25 
    int t=n; //t=5 
    while(t>0) 5>0 condition statisfy
    {
        if(t%10!=s%10)5!=5 condition fails
        {
           flag=1;
           break;
        }
        t=t/10; 5/10=0
        s=s/10;25/10=2
    }
    if(flag==1)//condition fails
    cout<<"Not an Automorphic Number";
    else
    cout<<"Automorphic Number";
    return 0;
}
