#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n],i,cnt=0;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    double avg=0;
    for(i=0;i<n;i++)
    {
        avg=avg+a[i];
    }
    avg=avg/n;
    for(i=0;i<n;i++)
    {
    if(a[i]<=avg)
    cnt++;
    }
    cout<<cnt;
}
