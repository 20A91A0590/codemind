#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n],i,j,sum=0,x,y,cnt=0;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cin>>x>>y;
    for(i=0;i<n;i++)
    {
        if(a[i]>=x && a[i]<=y)
        {
            sum=sum+a[i];
        }
    }
cout<<sum;
}
