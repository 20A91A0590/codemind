#include<bits/stdc++.h>
using namespace std;
int main()
{
    int num,r,L=0;//num=9584
    cin>>num;
while (num > 0)9584>0 958>0 95>0 9>0 condition statisfy
    {
r = num % 10; 9584%10=4 958%10=8 95%10=5 9%10=9
if (L<r)//0<4 4<8 8<9 condition statisfy 8<5 condition fail
 {
     L= r;//l=4 l=8 l=9
 }
num = num / 10;9584/10=958 958/10=95 95/10=9 9/10=0 out of the loop
    }
cout<< L;//l=9
return 0;
}
