#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n],i,cnt=0;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
    if(a[i]%2!=0)
    {
        cnt++;
       break;
    }
    }
    if(cnt==0)
    {
        cout<<"True"; 
    }
    else
    {
        cout<<"False";
    }
}
