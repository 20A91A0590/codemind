#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,sum=0,r;//n=1234
    cin>>n;
    while(n!=0)//1234!=0 condition statisfy
    {
        r=n%10;//1234%10=4 123%10=3  12%10=2 1%10=1
        sum=sum*10+r; 0+4 40+3=43 430+2=432 4320+1=4321
        n=n/10;1234/10=123 123/10=12 12/10=1  1/10=0
    }
    cout<<sum;
}
