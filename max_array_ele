#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int i,a[n],cnt=0,max=0;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
        if(max<=a[i])
        {
            max=a[i];
            cnt++;
        }
    }
    if(cnt>0)
    cout<<max;
    else
    cout<<"-1";
}
