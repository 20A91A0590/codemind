#include<bits/stdc++.h>
using namespace std;
int main()
{
    int N;
    cin>>N;
    int a[N],i,cnt=0,count=0;
    for(i=0;i<N;i++)
    {
        cin>>a[i];
    }
        for(i=0;i<N-1;i++)
    {
        if(a[i]<a[i+1])
        {
          count++;
        }
        else
        {
            cnt++;
        }
    }
    if(cnt>0)
    cout<<"no";
    else
    cout<<"yes";
}
