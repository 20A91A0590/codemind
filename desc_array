#include<bits/stdc++.h>
using namespace std;
int main()
{
    int N;
    cin>>N;
    int a[N],i,cnt=0;
    for(i=0;i<N;i++)
    {
        cin>>a[i];
    }
        for(i=0;i<=N;i++)
    {
        if(a[i]>a[i+1])
        {
          cnt++;
        }
    }
    if(cnt==(N-1))
    cout<<"no";
    else
    cout<<"yes";
}
